{
  "name": "fire-assets",
  "version": "0.3.0",
  "description": "Fireball builtin assets explorer",
  "author": "Firebox Technology",
  "type": "fireball",
  "main": "editor/main.js",
  "menus": {
    "Panels/Assets": {
      "message": "fire-assets:open"
    }
  },
  "panels": {
    "panel": {
      "frame": "editor/panel/fire-assets.html",
      "type": "dockable",
      "ctor": "FireAssets",
      "title": "Assets",
      "popable": false,
      "width": 200,
      "messages": [
        "asset:hint",
        "folder:created",
        "asset:moved",
        "assets:created",
        "assets:deleted",
        "fire-assets:refresh-context-menu",
        "selection:asset:selected",
        "selection:asset:unselected",
        "selection:asset:activated",
        "selection:asset:deactivated"
      ],
      "profiles": {
        "local": {
          "CollapsesStatus": []
        }
      }
    }
  }
}
