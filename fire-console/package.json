{
  "name": "fire-console",
  "version": "0.3.0",
  "description": "Fireball builtin console view",
  "author": "Firebox Technology",
  "fireball": {
    "main": "editor/main.js",
    "menus": [
      {
        "path": "View/Panels/Console",
        "message": "fire-console:open"
      }
    ],
    "panels": {
      "default": {
        "type": "dockable",
        "view": "editor/panel/fire-console.html",
        "ctor": "FireConsole",
        "title": "Console",
        "height": 300,
        "messages": [
          "console:log",
          "console:success",
          "console:failed",
          "console:info",
          "console:warn",
          "console:error"
        ]
      }
    }
  }
}