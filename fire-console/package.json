{
  "name": "fire-console",
  "version": "0.3.0",
  "description": "Fireball builtin console view",
  "author": "Firebox Technology",
  "type": "fireball",
  "main": "editor/main.js",
  "menus": {
    "View/Panels/Console": {
      "message": "fire-console:open"
    }
  },
  "panels": {
    "panel": {
      "type": "dockable",
      "view": "editor/panel/fire-console.html",
      "ctor": "FireConsole",
      "title": "Console",
      "height": 300,
      "shortcuts": {
        "command+k": "clear"
      },
      "messages": [
        "console:log",
        "console:success",
        "console:failed",
        "console:info",
        "console:warn",
        "console:error"
      ]
    }
  }
}
