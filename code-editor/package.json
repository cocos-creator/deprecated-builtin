{
  "name": "code-editor",
  "version": "0.1.0",
  "description": "The code editor plugin for Fireball Editor",
  "author": "Firebox Technology",
  "fireball": {
    "main": "editor/main.js",
    "panels": {
      "default": {
        "type": "dockable",
        "view": "editor/panel/code-editor.html",
        "ctor": "CodeEditor",
        "title": "Code Editor",
        "width": 1200,
        "height": 600,
        "min-width": 400,
        "min-height": 400,
        "messages": [
          "panel:open",
          "asset:changed"
        ]
      }
    }
  }
}
