{
  "name": "fire-inspector",
  "version": "0.3.0",
  "description": "Fireball builtin inspector",
  "author": "Firebox Technology",
  "type": "fireball",
  "main": "editor/main.js",
  "menus": {
    "View/Panels/Inspector": {
      "message": "fire-inspector:open"
    }
  },
  "panels": {
    "panel": {
      "type": "dockable",
      "view": "editor/panel/fire-inspector.html",
      "ctor": "FireInspector",
      "title": "Inspector",
      "popable": false,
      "width": 300,
      "min-width": 240,
      "messages": [
        "asset:changed",
        "asset:moved",
        "asset:saved",
        "component:added",
        "component:removed",
        "selection:activated"
      ]
    }
  }
}
